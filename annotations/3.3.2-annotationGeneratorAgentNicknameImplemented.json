    "properties": 
    {  "generator":
        { "oneOf":
           [                
                { "$ref": "otherProperties.json#/definitions/agentSingularNicknameFound" },
                { "type": "array",
                  "minItems": 1, 
                  "not": 
                  { "items": { "not": { "$ref": "otherProperties.json#/definitions/agentSingularNicknameFound" } } }
                }
           ]
        }
    }, 
    "required": ["generator"] 
