    "bodiesTargets/3.3.1-bodyModifiedValidated.json",
    "bodiesTargets/3.3.6-bodyRightsValidated.json",
    "bodiesTargets/3.3.7-bodyCanonicalValidated.json",
    "bodiesTargets/3.3.7-bodyViaValidated.json",
    "bodiesTargets/3.2.7-bodyEWRNoItems.json",
    "bodiesTargets/3.3.5-bodyEWRNoPurpose.json",
    "bodiesTargets/3.2.4-bodyChoiceSetNoValue.json",
    "bodiesTargets/4-bodyChoiceSetNoSource.json",
    "bodiesTargets/3.3.5-bodyChoiceSetNoPurpose.json",
    "bodiesTargets/3.2.7-bodyEmbeddedTextualNoItems.json",
    "bodiesTargets/4-bodyEmbeddedTextualNoSource.json",
