  "ref": "https://www.w3.org/TR/annotation-model/#bodies-and-targets",
  "assertions": [
    "annotations/3.2-bodyObjectsRecognized.json",
    "bodiesTargets/3.2.1-bodyTextDirectionValidated.json",
